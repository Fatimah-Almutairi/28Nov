generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")

}

model Users {
  id        String  @id @default(uuid())
  username  String  @unique
  password  String
  email     String   @unique
}

model Student {
  id String @id @default(uuid())
  name String 
  age Int
  major String 
}

model Classroom {
  id String @id @default(uuid())
  name String 
}

model Teacher {
  id String @id @default(uuid())
  name String 
}
