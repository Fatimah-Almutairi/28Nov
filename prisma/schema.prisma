generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
}

model Users {
  id String  @id @default(uuid())
  username String 
  password  String
}

model Book {
  id String @id @default(uuid())
  name String
  genre String
}

model Loan {
  id String @id @default(uuid())
  user_id String
  book_id String 
}